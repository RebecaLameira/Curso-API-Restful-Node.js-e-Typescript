{
  "name": "backend-typescript",
  "version": "1.0.0",
  "description": "Typescript with NodeJs",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --inspect --ignore-watch node_modules src/server.ts",
    "knex:migrate": "knex --knexfile knexfile.ts migrate:latest",
    "knex:seed": "knex --knexfile knexfile.ts seed:run"
  },
  "keywords": [
    "node.js",
    "typescript"
  ],
  "author": "Rebeca Lameira",
  "license": "ISC",
  "devDependencies": {
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.13",
    "bcryptjs": "^2.4.3",
    "celebrate": "^13.0.3",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "knex": "^0.21.6",
    "multer": "^1.4.2",
    "sqlite3": "^5.0.0"
  }
}
